{
  "name": "NB-IoT",
  "title": "NB-IoT",
  "description": "Quectel NB-IoT development platform",
  "url": "https://github.com/OS-Q/H11",
  "homepage": "https://github.com/OS-Q",
  "license": " ",
  "engines": { "platformio": "^3.0.0" },
  "repository": {
    "type": "git",
    "url": "https://github.com/OS-Q/H11.git"
  },
  "version": "1.0.0",
  "packageRepositories": [
    "https://dl.bintray.com/platformio/dl-packages/manifest.json",
    "http://dl.platformio.org/packages/manifest.json"
  ],
  "frameworks": {
      "opencpu": {
        "package": "framework-NB",
        "script": "builder/frameworks/opencpu.py"
      },
      "openlinux": {
        "package": "framework-NB",
        "script": "builder/frameworks/openlinux.py"
      },
      "threadx": {
        "package": "framework-NB",
        "script": "builder/frameworks/threadx.py"
      },
      "arduino": {
        "package": "framework-NB",
        "script": "builder/frameworks/arduino.py"
      },
      "wizio": {
        "package": "framework-NB",
        "script": "builder/frameworks/wizio.py"
      }      
  },
  "packages": {
    "framework-NB": {
        "type": "framework",
        "version": "https://github.com/OS-Q/N15.git"
    },
    "toolchain-gccarmnoneeabi": {
        "type": "toolchain",
        "version": "=1.70201.0"
    },
    "tool-quectel": {
        "type": "uploader",
        "version": "https://github.com/Wiz-IO/tool-quectel.git",
        "optional": true
    }
  }
}
