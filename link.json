{
  "name": "P67",
  "title": "Quectel",
  "description": "Quectel development platform",
  "url": "https://github.com/OS-Q/P67",
  "homepage": "http://www.wizio.eu/",
  "license": "Apache-2.0",
  "engines": {
    "platformio": "^5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OS-Q/P67.git"
  },
  "version": "2.1.3",
  "frameworks": {
      "opencpu": {
        "package": "framework-quectel",
        "script": "extend/opencpu.py"
      },
      "openlinux": {
        "package": "framework-quectel",
        "script": "extend/openlinux.py"
      },
      "threadx": {
        "package": "framework-quectel",
        "script": "extend/threadx.py"
      },
      "arduino": {
        "package": "framework-quectel",
        "script": "extend/arduino.py"
      }
  },
  "packages": {
    "framework-quectel": {
        "type": "framework",
        "version": "https://github.com/Wiz-IO/framework-quectel.git"
    },
    "toolchain-gccarmnoneeabi": {
        "type": "toolchain",
        "version": "=1.70201.0"
    },
    "toolchain-gcc-ec2x": {
        "type": "toolchain",
        "version": "https://github.com/Wiz-IO/LIB/raw/master/quectel/toolchain-gcc-ec2x-v1.0.0.zip"
    },
    "tool-quectel": {
        "type": "uploader",
        "version": "https://github.com/Wiz-IO/tool-quectel.git",
        "optional": true
    }
  }
}
